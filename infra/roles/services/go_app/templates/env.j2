# Managed by Ansible - environment configuration for {{ go_app_service_name }}
APP_URL={{ go_app_env_map.APP_URL | default('') }}
JWT_SECRET={{ go_app_env_map.JWT_SECRET | default('') }}
REDIS_URL={{ go_app_env_map.REDIS_URL | default('') }}
GEOIP_ENDPOINT={{ go_app_env_map.GEOIP_ENDPOINT | default('') }}
{% for key, value in (go_app_env_map | dictsort) if key not in ['APP_URL', 'JWT_SECRET', 'REDIS_URL', 'GEOIP_ENDPOINT'] %}
{{ key }}={{ value }}
{% endfor %}
