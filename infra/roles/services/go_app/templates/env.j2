# Managed by Ansible - environment configuration for {{ go_app_service_name }}
{% set ordered_keys = [
  'HTTP_ADDR',
  'APP_URL',
  'AUTH_BASE_URL',
  'SITES_BASE_URL',
  'JOBS_BASE_URL',
  'JWT_SECRET',
  'JWT_AUDIENCE',
  'JWT_ISSUER',
  'REDIS_URL',
  'GEOIP_ENDPOINT',
  'CORS_ALLOWED_ORIGINS',
  'RATE_ENABLED',
  'RATE_RPS',
  'RATE_BURST',
  'RATE_SKIP_AGENTS',
  'BASIC_TO_JWT_ENABLED',
  'LOGIN_TIMEOUT_SEC'
] %}
{% for key in ordered_keys if key in go_app_env_map %}
{{ key }}={{ go_app_env_map[key] }}
{% endfor %}
{% for key, value in (go_app_env_map | dictsort) if key not in ordered_keys %}
{{ key }}={{ value }}
{% endfor %}
